{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["es2022", "DOM", "DOM.Iterable"],
    "module": "esnext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noImplicitAny": true,
    "jsx": "react",
    "strict": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["srcjs"],
}
